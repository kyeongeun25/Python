alist1 = [30, 30, 52, 34, 43, 22, 23, 44, 55,6]

# alist1의 요소에 9의 값을 덧셈 뺄셈 곱셈 나눗셈을 표시하고 싶다.
for i in alist1 :
    print("덧셈",i + 9,end=",")
    print("뺄셈",i - 9,end=",")
    print("곱셈",i * 9,end=",")
    print("나눗셈",i / 9,end=",")
    
alist2 = [3,4,5,2,34,2,3,42,4]
for i in alist1 :
    print("덧셈",i + 9,end=",")
    print("뺄셈",i - 9,end=",")
    print("곱셈",i * 9,end=",")
    print("나눗셈",i / 9,end=",")
    
alist3 = [3,4,5,2,34,2,3,42,4]
for i in alist1 :
    print("덧셈",i + 9,end=",")
    print("뺄셈",i - 9,end=",")
    print("곱셈",i * 9,end=",")
    print("나눗셈",i / 9,end=",")